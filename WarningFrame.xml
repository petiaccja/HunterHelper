<Ui xmlns="http://www.blizzard.com/wow/ui/" 
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                   xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
                   ..\..\FrameXML\UI.xsd">
  <Script File="WarningFrame.lua"/>
  <Frame name="SharpShooter_WarningFrame">
    <Size x="256" y="64"/>
    <Anchors>
      <Anchor point="CENTER"  relativeTo="SharpShooter_SuggestionFrame" relativePoint="CENTER"/>
    </Anchors>
    <Layers>
      <Layer level="OVERLAY">
        <Texture name="SharpShooter_BlockSerpent" file="Interface/Addons/SharpShooter/graphics/block_sign_2.blp">
          <Size x="48" y="48"/>
          <Anchors>
            <Anchor point="CENTER" relativeTo="SharpShooter_SuggestSerpent" relativePoint="CENTER"/>
          </Anchors>
        </Texture>
        <Texture name="SharpShooter_BlockAimed" file="Interface/Addons/SharpShooter/graphics/block_sign_2.blp">
          <Size x="48" y="48"/>
          <Anchors>
            <Anchor point="CENTER" relativeTo="SharpShooter_SuggestAimed" relativePoint="CENTER"/>
          </Anchors>
        </Texture>
        <Texture name="SharpShooter_BlockMulti" file="Interface/Addons/SharpShooter/graphics/block_sign_2.blp">
          <Size x="48" y="48"/>
          <Anchors>
            <Anchor point="CENTER" relativeTo="SharpShooter_SuggestMulti" relativePoint="CENTER"/>
          </Anchors>
        </Texture>
        <Texture name="SharpShooter_WarnMulti" file="Interface/Addons/SharpShooter/graphics/warning_sign_2.blp">
          <Size x="48" y="48"/>
          <Anchors>
            <Anchor point="CENTER" relativeTo="SharpShooter_SuggestMulti" relativePoint="CENTER"/>
          </Anchors>
        </Texture>
      </Layer>
    </Layers>
    <Scripts>
      <OnLoad>
        SharpShooter_WF:OnLoad()
      </OnLoad>
    </Scripts>
  </Frame>
</Ui>